<bg xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='https://monkeymanboy.github.io/BSML-Docs/BSMLSchema.xsd'>
	<settings-container>
		<checkbox text="Enabled" value="enabled" apply-on-change="true"/>
		<checkbox text="Use Saber Color Scheme in Game" value="colScheme" apply-on-change="true"/>
		<checkbox text="Use Saber Color Scheme in Results Hover Hint" value="colSchemeInHint" apply-on-change="true"/>
		<checkbox text="Show Combo Drops in Results Hover Hint" value="comboDropsInHint" apply-on-change="true"/>
	</settings-container>
</bg>